{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bLog":1,
	"bStdErr":0,
}

{
	"name":"console",
	"class":"_Console",
	"bON":1,
    {
		"name":"thread",
		"FPS":10,
	}
	"vBASE":["cam","FB","state","arUco","TOFsense","/apBase","apLand","/serialTelem","apVideo","cURL"],
}

{
	"name":"CVwindow",
	"class":"_WindowCV",
	"bON":1,
	"bFullScreen":0,
    {
		"name":"thread",
		"FPS":30,
	}
	"vBASE":["/cam","camCrop","/univTag","arUco","apLand"],
	"bShow":1,
}

{
	"name":"FB",
	"class":"_GstOutput",
	"bON":0,
    {
		"name":"thread",
		"FPS":2,
	}
	"vBASE":["camCrop"],
	"gstOutput":"appsrc ! v4l2convert ! fbdevsink",
	"vSize":[1280,720],
}

{
	"name":"state",
	"class":"_StateControl",
    {
		"name":"thread",
		"FPS":10,
	}
	"bON":1,
	"/start":"STANDBY",
	"start":"LANDING",

	{
		"name":"state",
		{
			"bON":1,
			"name":"STANDBY",
			"class":"State",
		}
		{
			"bON":1,
			"name":"TAKEOFF_REQUEST",
			"class":"State",
		}
		{
			"bON":1,
			"name":"TAKEOFF_READY",
			"class":"State",
		}
		{
			"bON":1,
			"name":"AIRBORNE",
			"class":"State",
		}
		{
			"bON":1,
			"name":"LANDING_REQUEST",
			"class":"State",
		}
		{
			"bON":1,
			"name":"LANDING",
			"class":"State",
		}
		{
			"bON":1,
			"name":"TOUCHDOWN",
			"class":"State",
		}
		{
			"bON":1,
			"name":"LANDED",
			"class":"State",
		}
	}
}

