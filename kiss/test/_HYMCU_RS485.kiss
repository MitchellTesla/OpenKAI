{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bLog":0,
	"bStdErr":1,
}

{
	"name":"console",
	"class":"_Console",
	"bON":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"vBASE":["modbus","stepper"],
}

{
	"name":"modbus",
	"class":"_Modbus",
	"bON":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"bLog":0,
	"port":"/dev/ttyUSB0",
	"baud":9600,
	"parity":"N",	
}

{
	"name":"stepper",
	"class":"_HYMCU_RS485",
	"bON":1,
    {
		"name":"thread",
		"FPS":5,
	}
	"bLog":1,
	"_Modbus":"modbus",
	"iSlave":1,
	"dpr":1,
	"bFeedback":1,
	"dInit":1,

	{
		"name":"addr",
		"setDPR":4,
		"setDist":9,
		"setDir":11,
		"setSpd":8,
		"setAcc":2,
		"bComplete":12,
		"readStat":22,
		"run":7,
		"stop":3,
		"resPos":10,
	}

	{
		"name":"axis",
		{
			"name":"X",
			"pOrigin":0.0,
			"pTarget":0.0,
			"pErr":0.01,
			"pRange":[0,15],
			"sTarget":20.0,
			"sRange":[0,300],
			"aTarget":10.0,
			"aRange":[10,300],
		}
	}
}

{
	"name":"stepper",
	"class":"_HYMCU_RS485",
	"bON":0,
    {
		"name":"thread",
		"FPS":5,
	}
	"bLog":1,
	"_Modbus":"modbus",
	"iSlave":1,
	"dpr":1,
	"bFeedback":1,
	"dInit":-20,

	{
		"name":"axis",
		{
			"name":"X",
			"pOrigin":0.0,
			"pTarget":0.0,
			"pErr":0.01,
			"pRawRange":[0,15],
			"sTarget":1.0,
			"sRawRange":[0,300],
			"aTarget":0.0,
			"aRawRange":[10,300],
		}
	}
}

{
	"name":"gripper",
	"class":"_StepperGripper",
	"bON":0,
    {
		"name":"thread",
		"FPS":5,
	}
	"bLog":1,
	"_Modbus":"modbus",
	"iSlave":1,
	"dpr":1,
	"bFeedback":1,
	"dInit":-20,
	"pOpen":0,
	"pClose":1,
	"bOpen":1,

	{
		"name":"axis",
		{
			"name":"X",
			"pOrigin":0.0,
			"pTarget":0.0,
			"pErr":0.01,
			"pRawRange":[0,15],
			"sTarget":1.0,
			"sRawRange":[0,300],
			"aTarget":0.0,
			"aRawRange":[10,300],
		}
	}
}
