{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bWindow":1,
	"bDraw":1,
	"bLog":0,
	"bStdErr":1,
	"waitKey":30,
}

{
	"name":"OK_Console",
	"class":"_Console",
	"bON":1,
}

{
	"name":"apSerial",
	"class":"_SerialPort",
	"bON":1,
	"FPS":30,
	"bLog":0,
	"Window":"OKview",
	"_Console":"OK_Console",
	"port":"/dev/ttyACM0",
	"baud":115200,
	"/port":"/dev/ttyTHS1",
	"/port":"/dev/ttyAMA0",
	"/baud":921600,
	"_wakeUp":"mavAP",
}

{
	"name":"mavAP",
	"class":"_Mavlink",
	"bON":1,
	"FPS":30,
	"bLog":0,
	"Window":"OKview",
	"_Console":"OK_Console",
	"_IOBase":"apSerial",
	
	{
		"bON":1,
		"_Mavlink":"mavGCS",
	}
	{
		"bON":0,
		"_Mavlink":"mavWS",
	}
}

{
	"name":"apUDP",
	"class":"_UDP",
	"bON":1,
	"FPS":30,
	"bLog":0,
	"Window":"OKview",
	"_Console":"OK_Console",
	"port":19840,
	"_wakeUp":"mavGCS",
}

{
	"name":"mavGCS",
	"class":"_Mavlink",
	"bON":1,
	"FPS":30,
	"bLog":0,
	"Window":"OKview",
	"_Console":"OK_Console",
	"_IOBase":"apUDP",
	
	{
		"bON":1,
		"_Mavlink":"mavAP",
	}
	
	{
		"bON":0,
		"_Mavlink":"mavWS",
	}
}

{
	"name":"apWS",
	"class":"_WebSocket",
	"bON":0,
	"FPS":30,
	"Window":"OKview",
	"_Console":"OK_Console",
}

{
	"name":"mavWS",
	"class":"_Mavlink",
	"bON":0,
	"FPS":30,
	"bLog":0,
	"Window":"OKview",
	"_Console":"OK_Console",
	"_IOBase":"apWS",
	
	{
		"bON":1,
		"_Mavlink":"mavGCS",
	}
	
	{
		"bON":1,
		"_Mavlink":"mavAP",
	}
}
